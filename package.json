{
	"name": "highspell-hub",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"fetch-assets": "node --loader=ts-node/esm --experimental-specifier-resolution=node bin/fetch-assets.ts",
		"cache-assets": "node --loader=ts-node/esm --experimental-specifier-resolution=node bin/cache-assets.ts",
		"compile-caches": "node --loader=ts-node/esm --experimental-specifier-resolution=node bin/compile-caches.ts",
		"copy-caches": "mkdir -p static/client-caches && cp -r cache/** static/client-caches",
		"compile-item-defs": "node --loader=ts-node/esm --experimental-specifier-resolution=node bin/compile-item-defs.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.26",
		"@sveltejs/adapter-vercel": "^1.0.0-next.40",
		"@sveltejs/kit": "next",
		"@types/lodash": "^4.14.178",
		"@types/sharp": "^0.29.5",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"node-fetch": "^3.2.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"sass": "^1.49.0",
		"sharp": "^0.29.3",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.2",
		"ts-node": "^10.4.0",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "^5.15.4",
		"lodash": "^4.17.21",
		"lodash-es": "^4.17.21",
		"svelte-fa": "^2.4.0"
	}
}
